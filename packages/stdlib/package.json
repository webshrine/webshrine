{
  "name": "@webshrine/stdlib",
  "type": "module",
  "version": "0.0.0-alpha.4",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webshrine/webshrine.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "test": "vitest --watch --typecheck",
    "test:coverage": "vitest --coverage",
    "dev": "vite --host",
    "check_types": "tsc",
    "build_soft": "vite build",
    "build": "npm run check_types && vite build",
    "bench": "vitest bench --watch",
    "deps:check_updates": "ncu",
    "deps:update": "ncu -u && pnpm i",
    "release": "npm run build && ..."
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@webshrine/stdtyp": "link:..\\stdtyp",
    "cope": "^1.0.9",
    "debounce": "^2.2.0",
    "fast-deep-equal": "^3.1.3",
    "klona": "^2.0.6",
    "lodash-es": "^4.17.21",
    "throttleit": "^2.1.0",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.36.0",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.6"
  }
}
