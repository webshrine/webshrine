{
  "name": "@webshrine/stdlib",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "jsdelivr": "./dist/index.iife.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "test": "vitest --watch --typecheck",
    "test:coverage": "vitest --coverage",
    "dev": "vite --host",
    "check_types": "tsc",
    "build_soft": "vite build",
    "build_strict": "npm run check_types && vite build",
    "bench": "vitest bench --watch",
    "deps:check_updates": "ncu",
    "deps:update": "ncu -u && pnpm i",
    "release": "npm run build && ..."
  },
  "dependencies": {
    "@types/lodash": "^4.17.13",
    "@webshrine/stdtyp": "link:..\\stdtyp",
    "cope": "^1.0.9",
    "eventemitter3": "^5.0.1",
    "klona": "^2.0.6",
    "lodash": "^4.17.21",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "terser": "^5.36.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5"
  }
}
