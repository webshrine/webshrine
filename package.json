{
  "name": "webshrine",
  "version": "0.0.12",
  "packageManager": "pnpm@9.12.1",
  "description": "Standard libraries for unified web development",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webshrine/webshrine.git"
  },
  "keywords": [],
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "build": "pnpm -r run build",
    "docs:prepare": "typedoc && bun ./scripts/parseGeneratedApiDoc.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run docs:prepare && vitepress build docs",
    "docs:preview": "npm run docs:prepare && vitepress preview docs",
    "deps:check_updates": "ncu && ncu -ws",
    "deps:update": "ncu -u && ncu -ws -u && pnpm i",
    "script:generateAutoCategories": "bun scripts/generateAutoCategories.ts && npm run docs:prepare",
    "script:commitVersion": "bun scripts/commitVersion.ts",
    "git:push_all": "git push origin main && git push origin --tags",
    "ci:publish": "bun scripts/publish.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.2",
    "@vueuse/core": "^12.0.0",
    "consola": "^3.2.3",
    "dependency-cruiser": "^16.8.0",
    "echarts": "^5.5.1",
    "eslint": "^9.17.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.83.0",
    "terser": "^5.37.0",
    "ts-morph": "^24.0.0",
    "typedoc": "^0.27.5",
    "typedoc-plugin-markdown": "^4.3.2",
    "typedoc-vitepress-theme": "^1.1.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0",
    "vitepress": "^1.5.0",
    "vitepress-plugin-npm-commands": "^0.6.0",
    "vitepress-plugin-tabs": "^0.5.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13"
  }
}
