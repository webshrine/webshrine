{
  "name": "webshrine",
  "version": "0.2.0",
  "packageManager": "pnpm@10.19.0",
  "description": "Standard libraries for unified web development",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webshrine/webshrine.git"
  },
  "keywords": [],
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "build": "pnpm -r run build",
    "docs:prepare": "typedoc && bun ./scripts/parseGeneratedApiDoc.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run docs:prepare && vitepress build docs",
    "docs:preview": "npm run docs:prepare && vitepress preview docs",
    "deps:check_updates": "ncu && ncu -ws",
    "deps:update": "ncu -u && ncu -ws -u && pnpm i",
    "script:generateAutoCategories": "bun scripts/generateAutoCategories.ts && npm run docs:prepare",
    "script:commitVersion": "bun scripts/commitVersion.ts",
    "git:push_all": "git push origin main && git push origin --tags",
    "ci:publish": "bun scripts/publish.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@types/bun": "^1.3.0",
    "@types/node": "^22.10.2",
    "@vueuse/core": "^14.0.0",
    "consola": "^3.4.2",
    "dependency-cruiser": "^17.1.0",
    "echarts": "^6.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-jsdoc": "^61.1.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.93.2",
    "terser": "^5.44.0",
    "ts-morph": "^27.0.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vitepress-plugin-npm-commands": "^0.8.1",
    "vitepress-plugin-tabs": "^0.7.3",
    "vitest": "^4.0.1",
    "vue": "^3.5.22"
  }
}
