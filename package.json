{
  "name": "webshrine",
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.1",
  "description": "Standard libraries for unified web development",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "docs:prepare": "typedoc",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run docs:prepare && vitepress build docs",
    "docs:preview": "npm run docs:prepare && vitepress preview docs",
    "deps:check_updates": "ncu -ws",
    "deps:update": "ncu -ws -u && ncu -u && pnpm i"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "eslint": "^9.15.0",
    "typedoc": "^0.26.11",
    "typedoc-plugin-markdown": "^4.2.10",
    "typedoc-vitepress-theme": "^1.0.2",
    "typescript": "^5.6.3",
    "vitepress": "^1.5.0"
  }
}
